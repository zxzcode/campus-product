<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kmbeast.mapper.OrderMapper">
    <insert id="save"></insert>
    <select id="findById">
        select *
        from orders
        where id=#{id}
    </select>
</mapper>